<section class="dashboard">
  <sidebar>
    <%= image_tag(('default-teacher.png'), class: 'avatar') %>
    <h3 class="sidebar-name"><%= @user.first_name %></h3>
    <ul class="profile content">
      <li>
        <p>name: <%= @user.name %></p>
      </li>
      <li>
        <p>email: <%= @user.email %></p>
      </li>

    </ul>
  </sidebar>
  <% @user.courses.each do |course| %>
    <li> <%= link_to course.name, course %></li>
  <%end%>
</section>
<div id="chart"></div>
